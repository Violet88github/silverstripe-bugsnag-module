{
    "name": "violet88/bugsnag-silverstripe",
    "description": "A module to use Bugsnag in SilverStripe projects.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "BugSnag",
        "violet88"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "bugsnag/bugsnag": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "bugsnag/bugsnag-psr-logger": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Violet88\\BugsnagModule\\": "src/",
            "Violet88\\BugsnagModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
